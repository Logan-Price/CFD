FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    location        system;
    object          probesDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


// Fields to be probed. runTime modifiable!
fields
(
    U p 
);

// Locations to be probed. runTime modifiable!
probeLocations
(
    (1.24 0.0 0.0)
);

